.tabber{
  width: 100%;
  height: 40px;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 1003;
  background: #fff;
  padding: 5px 0;
  border-top: 1px solid #f4f4f4;
}
.tabber .flex-item image{
  width: 50rpx;
  height: 50rpx;
  line-height: 1;
}
.flex {
  display: flex;
}

.flex .flex-item {
  flex: 1;
  line-height: 1;
}
.tc{
  text-align: center;
}
.f12{
  font-size: 24rpx;
}
.f14{
  font-size: 28rpx;
}
.active{
  color: red;
}