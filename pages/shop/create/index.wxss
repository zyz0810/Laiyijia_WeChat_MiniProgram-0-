/* pages/shop/create/index.wxss */
page{
  background: #fff;
}
.submit_btn{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  margin: 200rpx auto;
  border-radius: 8rpx;
  background: #FEC965;
}
.choosebg {
  background: #fff;
  display: block;
  height: 100%;
  left: 0;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 120;
  overflow-y: scroll;
}

.citybg {
  z-index: 121;
}

.choosebg>view {
  padding: 20rpx 0 20rpx 2%;
  border-bottom: 1rpx solid #f1f1f1;
}